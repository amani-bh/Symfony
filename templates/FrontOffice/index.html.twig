                    <div class="col-lg-5">
                        <div class="section-title">
                            <h4>New Recipes</h4>
                        </div>
                    </div>
                </div>
                <div class="row property-filter">
                    {% for i in newRecipes %}
                        <div class="col-lg-4 col-md-6">
                            <div class="property-item">
                                <div class="pi-pic set-bg" data-setbg="{{ asset('assets/img/recipe/'~i.imgUrl) }}">
                                    <div class="label c-red">Task</div>
                                </div>
                                <div class="pi-text">
                                    <a href="#" class="heart-icon"><span class="icon_heart_alt"></span></a>
                                    <div class="pt-price">$ 289.0</div>
                                    <h5><a href="#">{{ i.title }}</a></h5>
                                    <p><span class="icon_pin_alt"></span> 12 Pt at Shedung, Panvel, Raigarh, Navi Mumbai</p>
                                    <ul>
                                        <li><i class="fa fa-object-group"></i> 2, 283</li>
                                        <li><i class="fa fa-bathtub"></i> 03</li>
                                        <li><i class="fa fa-bed"></i> 05</li>
                                    </ul>
                                    <div class="pi-agent">
                                        <div class="pa-item">
                                            <div class="pa-info">
                                                <img src="../assets/others/img/property/posted-by/pb-1.jpg" alt="">
                                                <h6>{{ i.user.firstName }}</h6>
                                            </div>
                                            <div class="pa-text">
                                                123-455-688
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    {% endfor %}
